                "READ_DATABASE_CONNECTION_STRING": "Host=localhost;port=5432;Database=SCTV;Username=postgres;Password=postgres",
                "ASPNETCORE_ENVIRONMENT": "Development",
                "REDIS_HOST": "127.0.0.1:6379,password=admin"
            }
        },
        "Streaming.API": {
            "commandName": "Project",
            "launchBrowser": true,
            "launchUrl": "swagger",
            "environmentVariables": {
                "WRITE_DATABASE_CONNECTION_STRING": "Host=127.0.0.1;port=5432;Database=SCTV;Username=postgres;Password=postgres",
                "READ_DATABASE_CONNECTION_STRING": "Host=127.0.0.1;port=5432;Database=SCTV;Username=postgres;Password=postgres",
