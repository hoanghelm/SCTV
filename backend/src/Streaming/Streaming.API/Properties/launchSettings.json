        "iisExpress": {
            "applicationUrl": "http://localhost:62162",
            "sslPort": 44323
        }
    },
    "$schema": "http://json.schemastore.org/launchsettings.json",
    "profiles": {
        "IIS Express": {
            "commandName": "IISExpress",
            "launchBrowser": true,
